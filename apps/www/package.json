{
  "name": "www",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "bun run registry:build && next build",
    "dev": "next dev --turbopack",
    "start": "next start",
    "postinstall": "fumadocs-mdx",
    "registry:build": "REGISTRY_HOST=https://limeplay.winoffrg.dev tsx --tsconfig ./tsconfig.scripts.json ./scripts/build-registry.ts && prettier --log-level error --write \"registry/**/*.{ts,tsx,json,mdx}\" --cache",
    "registry:dev": "REGISTRY_HOST=http://localhost:3000 tsx --watch --tsconfig ./tsconfig.scripts.json ./scripts/build-registry.ts && prettier --log-level error --write \"registry/**/*.{ts,tsx,json,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache --config ../../prettier.config.cjs && next lint",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache --config ../../prettier.config.cjs"
  },
  "dependencies": {
    "@base-ui-components/react": "1.0.0-beta.1",
    "@lottiefiles/dotlottie-react": "^0.14.4",
    "@phosphor-icons/react": "^2.1.10",
    "@radix-ui/react-compose-refs": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-navigation-menu": "^1.2.13",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.11",
    "fumadocs-core": "15.6.7",
    "fumadocs-mdx": "11.7.1",
    "fumadocs-typescript": "^4.0.6",
    "fumadocs-ui": "15.6.7",
    "jotai": "^2.12.5",
    "leva": "^0.10.0",
    "lodash.clamp": "^4.0.3",
    "lodash.throttle": "^4.1.1",
    "lucide-react": "^0.532.0",
    "motion": "^12.23.11",
    "next": "15.4.4",
    "next-themes": "^0.4.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "remark-code-import": "^1.2.0",
    "remark-gfm": "^4.0.1",
    "shaka-player": "^4.15.9",
    "tailwind-merge": "^3.3.1",
    "ts-morph": "^26.0.0",
    "tw-animate-css": "^1.3.6",
    "zod": "^3.24.2",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.5.1",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/lodash.clamp": "^4.0.9",
    "@types/lodash.throttle": "^4.1.9",
    "@types/mdx": "^2.0.13",
    "@types/node": "24.1.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "eslint": "^9",
    "eslint-config-next": "15.4.4",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "rehype-slug": "^6.0.0",
    "rimraf": "^6.0.1",
    "shadcn": "2.10.0",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3"
  },
  "prettier": {
    "endOfLine": "lf",
    "semi": false,
    "singleQuote": false,
    "tabWidth": 2,
    "trailingComma": "es5",
    "importOrder": [
      "^(react/(.*)$)|^(react$)",
      "^(next/(.*)$)|^(next$)",
      "<THIRD_PARTY_MODULES>",
      "",
      "^@workspace/(.*)$",
      "",
      "^types$",
      "^@/types/(.*)$",
      "^@/config/(.*)$",
      "^@/lib/(.*)$",
      "^@/hooks/(.*)$",
      "^@/components/ui/(.*)$",
      "^@/components/(.*)$",
      "^@/registry/(.*)$",
      "^@/styles/(.*)$",
      "^@/app/(.*)$",
      "^@/www/(.*)$",
      "",
      "^[./]"
    ],
    "importOrderSeparation": false,
    "importOrderSortSpecifiers": true,
    "importOrderBuiltinModulesToTop": true,
    "importOrderParserPlugins": [
      "typescript",
      "jsx",
      "decorators-legacy"
    ],
    "importOrderMergeDuplicateImports": true,
    "importOrderCombineTypeAndValueImports": true,
    "plugins": [
      "@ianvs/prettier-plugin-sort-imports"
    ]
  }
}
