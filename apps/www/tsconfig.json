{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ESNext",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "preserveSymlinks": false,
    "incremental": true,
    "paths": {
      "fumadocs-mdx:collections/*": [".source/*"],
      "@/*": ["./*"],
      "react": ["./node_modules/@types/react"]
    },
    "plugins": [
      {
        "name": "next"
      }
    ],
    "strictNullChecks": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "types",
    "registry/__index__.tsx",
    "postcss.config.mjs",
    "eslint.config.mjs",
    ".next/dev/types/**/*.ts",
    "next.config.mjs"
  ],
  "exclude": ["node_modules", ".next", "registry/__index__.tsx"]
}
